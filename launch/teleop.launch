<launch>
	<arg name="motor_port" />
	<node pkg="vnh5019_motor_controller" type="serial_transmitter" name="vnh5019_motor_controller" args="$(arg motor_port)" />
	<node pkg="vnh5019_motor_controller" type="keyboard_teleop" name="motor_key_teleop" />
	<node pkg="key_teleop" type="key_teleop.py" name="keyboard_teleop" output="screen"/>
</launch>
